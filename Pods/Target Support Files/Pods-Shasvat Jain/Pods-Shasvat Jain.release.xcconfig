FRAMEWORK_SEARCH_PATHS = "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/EAIntroView.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/EARestrictedScrollView.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/Shimmer.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "EAIntroView" -framework "EARestrictedScrollView" -framework "Shimmer"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Shasvat Jain
PODS_ROOT = ${SRCROOT}/Pods